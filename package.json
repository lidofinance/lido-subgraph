{
  "name": "lido-subgraph",
  "version": "2.1.17",
  "license": "MIT",
  "scripts": {
    "codegen": "rm -rf ./generated && graph codegen && node genParserData.js",
    "build": "graph build --network mainnet",
    "build-hoodi": "graph build --network hoodi",
    "test": "yarn test:errors && yarn test:sync && yarn test:withoutStatus",
    "test:hosted": "yarn test:errors && yarn test:sync && yarn test:withoutStatus --testPathIgnorePatterns addressShares balances",
    "test:withoutStatus": "NODE_OPTIONS=--experimental-vm-modules jest --testPathIgnorePatterns isSynced isErrored",
    "test:sync": "NODE_OPTIONS=--experimental-vm-modules jest tests/isSynced.test.js",
    "test:errors": "NODE_OPTIONS=--experimental-vm-modules jest tests/isNotErrored.test.js",
    "test:totals": "yarn test:sync && NODE_OPTIONS=--experimental-vm-modules jest tests/totalPooledEth.test.js tests/totalShares.test.js",
    "test:dust": "NODE_OPTIONS=--experimental-vm-modules jest tests/noWrongDust.test.js",
    "test:graphBalance": "NODE_OPTIONS=--experimental-vm-modules yarn jest --testMatch='**/tests/graphBalance.test.off.alt.js'",
    "test:smoke": "NODE_OPTIONS=--experimental-vm-modules jest tests/smoke.test.js",
    "unit-test": "graph test",
    "create-local": "env-cmd --use-shell graph create --node '$GRAPH_NODE' lidofinance/lido",
    "remove-local": "env-cmd --use-shell graph remove --node '$GRAPH_NODE' lidofinance/lido",
    "deploy-local": "env-cmd --use-shell graph deploy --node '$GRAPH_NODE' --ipfs '$GRAPH_IPFS' lidofinance/lido --network mainnet",
    "create-local-hoodi": "env-cmd --use-shell graph create --node '$GRAPH_NODE' lidofinance/lido-hoodi",
    "remove-local-hoodi": "env-cmd --use-shell graph remove --node '$GRAPH_NODE' lidofinance/lido-hoodi",
    "deploy-local-hoodi": "env-cmd --use-shell graph deploy --node '$GRAPH_NODE' --ipfs '$GRAPH_IPFS' lidofinance/lido-hoodi --network hoodi",
    "create-lido-hoodi": "env-cmd --use-shell graph create --access-token '$AUTH_TOKEN_HOODI' --node '$GRAPH_NODE_HOODI' lidofinance/lido-hoodi",
    "remove-lido-hoodi": "env-cmd --use-shell graph remove --access-token '$AUTH_TOKEN_HOODI' --node '$GRAPH_NODE_HOODI' lidofinance/lido-hoodi",
    "deploy-lido-hoodi": "env-cmd --use-shell graph deploy --access-token '$AUTH_TOKEN_HOODI' --node '$GRAPH_NODE_HOODI' --ipfs '$GRAPH_IPFS_HOODI' --headers '$IPFS_AUTH_HOODI' lidofinance/lido-hoodi --network hoodi -l v2.0.1",
    "deploy-chainstack": "env-cmd --use-shell graph deploy --node '$GRAPH_NODE_CHAINSTACK' --ipfs '$GRAPH_IPFS_CHAINSTACK' lido-mainnet --network mainnet",
    "deploy-thegraph": "graph deploy lido"
  },
  "dependencies": {
    "@graphprotocol/graph-cli": "^0.98.1",
    "@graphprotocol/graph-ts": "^0.38.2"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.8.4",
    "dotenv": "16.3.1",
    "env-cmd": "10.1.0",
    "ethers": "5.7.2",
    "graphql": "16.8.1",
    "graphql-request": "^7.1.0",
    "jest": "29.7.0",
    "matchstick-as": "0.6.0",
    "prettier": "3.0.3",
    "typescript": "5.2.2"
  },
  "subgraphBuildHash": {
    "thegraph-v2.1.17": "", // not deployed yet
    "thegraph-v2.1.16": "QmdSjtPFe2p8ABLYFx5TPbtgyj5tmmBmxVb4T8d32KR6UR",
    "chainstack-staging": "QmeTiPNC2pv2tmDojhnGn22BQm4G6f1mzdJucKwtNn76Z8",
    "local": "QmQc7mZCcwPE6hVKZ56JQCUkF7oBAzhcNktuUrqYGnoHvK"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
