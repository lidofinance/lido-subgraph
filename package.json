{
  "name": "lido-subgraph",
  "license": "UNLICENSED",
  "scripts": {
    "codegen": "graph codegen",
    "build": "graph build",
    "create": "graph create --node $GRAPH_API_URL lido-subgraph",
    "remove": "graph remove --node $GRAPH_API_URL lido-subgraph",
    "deploy": "graph deploy --node $GRAPH_API_URL --ipfs $IPFS_API_URL lido-subgraph",
    "create-local": "graph create --node http://localhost:8020/ lido-subgraph",
    "remove-local": "graph remove --node http://localhost:8020/ lido-subgraph",
    "deploy-local": "graph deploy --node http://localhost:8020/ --ipfs http://localhost:5001 lido-subgraph",
    "deploy-thegraph": "graph deploy --node https://api.thegraph.com/deploy/ --ipfs https://api.thegraph.com/ipfs/ lido-subgraph",
    "deploy-docker": "npm run create && npm run deploy"
  },
  "dependencies": {
    "@graphprotocol/graph-cli": "0.19.0",
    "@graphprotocol/graph-ts": "0.19.0",
    "strip-ansi": "^6.0.0"
  }
}
